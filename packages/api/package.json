  "devDependencies": {
    "@graphql-codegen/add": "^1.13.1",
    "@graphql-codegen/cli": "^1.20.1",
    "@graphql-codegen/introspection": "^1.13.1",
    "@graphql-codegen/schema-ast": "^1.13.1",
    "@graphql-codegen/typescript": "^1.20.2",
    "@graphql-codegen/typescript-resolvers": "^1.13.1",
