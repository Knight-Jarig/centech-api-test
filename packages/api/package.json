    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.1.0",
    "core-js": "^3.6.5",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "fork-ts-checker-webpack-plugin": "^4.1.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "jest-transform-graphql": "^2.1.0",
    "lint-staged": "^10.5.3",
    "nodemon": "^2.0.2",
    "prettier": "^2.2.1",
    "sonarqube-scanner": "^2.7.0",
    "terser-webpack-plugin": "^2.3.5",
    "ts-jest": "^26.4.4",
    "ts-loader": "^6.2.1",
    "ts-node": "^9.0.0",
    "typescript": "^3.8.3",
    "webpack": "^4.41.3",
    "webpack-cli": "^3.3.10",
    "webpack-node-externals": "^1.7.2"
  },
  "peerDependencies": {
    "@central-tech/operation": "file:../operation"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "yarn lint"
    ],
    "*.{graphql,json}": [
      "prettier --write"
    ]
  },
  "resolutions": {
    "axios": "^0.21.1"
  }
}