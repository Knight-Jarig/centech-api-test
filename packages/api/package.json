{
  "name": "@central-tech/api",
  "version": "2.29.0",
  "description": "Graphql headless",
  "main": "dist/index.js",
  "private": true,
  "files": [
    "schema.graphql"
  ],
  "scripts": {
    "clean": "tsc --build --clean",
    "prebuild": "yarn codegen",
    "build": "webpack",
    "watch": "concurrently -k -n graphql-codegen \"yarn codegen --watch\"",
    "dev": "concurrently -k -n nodemon \"nodemon\" \"yarn watch\"",
    "start": "node build/main.js",
    "start:cds:sit": "env-cmd -f environment/cds/falcon-api/.env.sit yarn start",
    "start:cds:uat": "env-cmd -f environment/cds/falcon-api/.env.uat yarn start",
    "start:cds:lte": "env-cmd -f environment/cds/falcon-api/.env.lte yarn start",
    "start:cds:prod": "env-cmd -f environment/cds/falcon-api/.env.prod yarn start",
    "dev:cds:sit": "env-cmd -f environment/cds/falcon-api/.env.sit yarn dev",
    "dev:cds:uat": "env-cmd -f environment/cds/falcon-api/.env.uat yarn dev",
    "dev:cds:lte": "env-cmd -f environment/cds/falcon-api/.env.lte yarn dev",
    "dev:cds:prod": "env-cmd -f environment/cds/falcon-api/.env.prod yarn dev",
    "test": "jest",
    "test:coverage": "yarn test --coverage",
    "test:report": "jest --no-colors --config ./jest.config.report.js",
    "codegen": "graphql-codegen --config codegen.yml",
    "sonar": "node sonar-project.js",
    "lint": "eslint --quiet --fix"
  },
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/centraldigital/centech-api.git"
  },
  "dependencies": {
    "@newrelic/apollo-server-plugin": "^0.1.3",
    "@sentry/node": "^6.2.0",
    "@sentry/tracing": "^6.2.0",
    "@sentry/webpack-plugin": "^1.13.0",
    "@types/lodash": "^4.14.165",
    "@types/luxon": "^1.25.0",
    "apollo-datasource": "^0.7.0",
    "apollo-datasource-rest": "^0.7.0",
    "apollo-link": "^1.2.14",
    "apollo-link-context": "^1.0.20",
    "apollo-link-error": "^1.1.13",
    "apollo-link-http": "^1.5.17",
    "apollo-server": "^2.9.9",
    "apollo-server-cache-redis": "^1.1.4",
    "apollo-server-errors": "^2.4.1",
    "apollo-server-express": "^2.9.9",
    "apollo-server-plugin-response-cache": "^0.6.0",
    "aws-param-store": "^3.2.0",
    "aws-sdk": "^2.902.0",
    "aws4": "^1.11.0",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.3.1",
    "class-transformer-validator": "^0.8.0",
    "class-validator": "^0.11.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dataloader": "^2.0.0",
    "dotenv": "^8.2.0",
    "elliptic": "^6.5.4",
    "env-cmd": "^10.1.0",
    "express": "^4.17.1",
    "firebase": "^8.2.0",
    "graphql": "^14.5.8",
    "graphql-depth-limit": "^1.1.0",
